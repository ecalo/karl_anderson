@charset "utf-8";
/* CSS Document */

/* Modal Design*/

/*#add_request_popup{ left:50%; margin-left:-319px; top:35%;}*/

.dialog{ width:639px; position:absolute; z-index:5000; display:none; left:50%; margin-left:-319px;}

.dialog.narrow{ width:508px; margin-left:-254px;}

.jqmOverlay {background:url(../images/modal_overlay.png);}

.dialog .modal_top, .dialog .modal_bottom {
	height: 7px;
	position: relative;
}

.dialog.request_modal .col_left{width:355px;}

.modal_top_left {
	background-image: url(../images/modal_blue_top_left.png);
	position: absolute;
	height: 7px;
	width: 7px;
	left: 0px;
	top: 0px;
}
.modal_top_right {
	background-image: url(../images/modal_blue_top_right.png);
	position: absolute;
	height: 7px;
	width: 7px;
	top: 0px;
	right: 0px;
}

.modal_heading{ position:relative; top:-4px; margin:0 0px -4px;  }

.modal_heading_top{ position:relative; height:4px; margin:0 3px;}

.modal_heading_top .heading_top_left{ position:absolute; top:0; left:0; background:url(../images/modal_heading_top_left.png) no-repeat; height:4px; width:4px;}

.modal_heading_top .heading_top_right{ position:absolute; top:0; right:0; background:url(../images/modal_heading_top_right.png) no-repeat; height:4px; width:4px;}

.modal_heading_top .heading_top_center{ background:url(../images/modal_heading_top_pattern.png) repeat-x; height:4px; margin:0 4px; }

.modal_heading_content{ background: url(../images/modal_heading_pattern.jpg) repeat-x #ffcc66; border-right:3px solid #66A0CF; border-left:3px solid #66A0CF;  }

.modal_heading_content p{line-height:35px; padding: 0 10px 4px; color:#d58f25; font-weight:bold; font-size:15pt; font-family:"Arial Black", Gadget, sans-serif;}

.modal_heading_content p.small{font-size:11pt; line-height:25px;}

.modal_heading_content .border{border-bottom:1px solid #de9e5e;}

.modal_heading_content p.yellow_text{ color:#ffffcc; font-size:11pt; line-height:20px; padding-bottom:5px;}

.modal_bottom_left {
	background-image: url(../images/modal_blue_bottom_left.png);
	position: absolute;
	height: 7px;
	width: 7px;
	left: 0px;
	bottom: 0px;
}
.modal_bottom_right {
	background-image: url(../images/modal_blue_bottom_right.png);
	position: absolute;
	height: 7px;
	width: 7px;
	right: 0px;
	bottom: 0px;
}
.modal_top_pattern {
	height: 4px;
	margin-right: 7px;
	margin-left: 7px;
	border-top: 3px solid #66a0cf;
	background-color:#fff;
	
}

.modal_bottom_pattern {
	height: 4px;
	margin-right: 7px;
	margin-left: 7px;
	border-bottom: 3px solid #66a0cf;
	background-color:#fff;
	
}
.dialog .modal_content {
	border:3px solid #66a0cf;
	border-bottom: none;
	border-top: none;
	padding:21px 18px 14px;
	background-color:#fff;
}

.dialog .col_left{ width:280px; float:left; position:relative;}

.dialog .col_right{ width:280px; float:right; position:relative;}

.dialog.request_modal .col_right{width:215px;}


.dialog.narrow .col_left, .dialog.narrow .col_right{ width:232px;}

.textfield {
	font-size: 17px;
	padding-top: 4px;
	padding-bottom: 4px;
	line-height:22px;
	font-weight: bold; font-style:italic;
	color:#cccccc;
	height:22px;
}

.dialog .textfield.full_width{ width:585px;} 

.dialog select{font-weight: bold; font-style:italic;color:#cccccc; font-size: 17px;}

.dialog .textfield, .dialog textarea{ margin-bottom:25px; padding:0 5px;}

.dialog.request_modal textarea{ margin-bottom:10px;}

.dialog .textfield:focus, .dialog textarea:focus, .dialog select:focus {color:#434343; }

.dialog textarea{ height:200px; width:100%; font-weight: bold; font-style:italic;
	color:#cccccc; font-size: 17px; padding:5px; width:585px;}
	
.dialog input.wide{ width:585px;}
	
	
.dialog .col_right .category.drop, .dialog .col_left .category.drop {
	width: 280px;
	float: none;
	margin-bottom:25px;
	top:0;
}

.dialog .col_left .category.group{ width:145px; top:0;}

.dialog .category.drop.full_width{ width:100%; margin-bottom:12px;}

#modal_info, #modal_info2, #modal_info3 {
	background-image: url(../images/info_btn.jpg);
	height: 23px;
	width: 21px;
	position: absolute;
	top: 2px;
	left: 115px;
	z-index:100;
}


.dialog input, .dialog select, .dialog textarea{ font-style:italic;}

.dialog .dialog_buttons{ height:50px;}

.dialog .blue_button{ margin:0 0 0 20px ; width: 102px;}

.dialog .dialog_buttons .blue_button, .dialog .dialog_buttons .grey_button{top:0;}

.dialog .blue_button input{ margin:0; background-color:transparent; border:none; width: 102px; height:30px; position:absolute; left:0; top:0; cursor:pointer;}

.dialog p.button_text{ line-height:30px; text-align:center; color:#fff; float:none; margin:0; cursor:pointer;}

.dialog .blue_button:hover .button_text{color:#AFD0EA;}

.dialog .blue_button, .dialog .grey_button{ float:right; 	position:relative;}

.span {font-size:14px;}




